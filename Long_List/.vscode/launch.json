    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch Arithmetic",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Arithmetic",
            "args": ["test_in.txt", "test_out.txt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },        
